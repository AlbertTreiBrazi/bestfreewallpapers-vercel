import React, { useState, useEffect } from 'react'
import { useAuth } from '@/contexts/AuthContext'
import { useTheme } from '@/contexts/ThemeContext'
import { supabase } from '@/lib/supabase'
import { Users, Crown, DollarSign, TrendingUp, Calendar, Settings, CheckCircle, XCircle, Clock, Image as ImageIcon, Megaphone, X, Gauge, Tag, Link, Zap, Server, Trash2, ChevronDown } from 'lucide-react'
import toast from 'react-hot-toast'
import { WallpaperManagement } from './WallpaperManagement'
import { BannerManagement } from '../premium/BannerManagement'
import { CategoriesManagement } from './CategoriesManagement'
import { CollectionsManagement } from './CollectionsManagement'
import { SlugManagement } from './SlugManagement'
import { AdSettingsManagement } from './AdSettingsManagement'
import { CacheManagement } from './CacheManagement'
import { AuthModal } from '../auth/AuthModal'

interface AdminStats {
  totalUsers: number
  premiumUsers: number
  pendingRequests: number
  totalRevenue: number
  newUsersLast30Days: number
  downloadsLast30Days: number
  totalDownloads: number
  unreadMessages: number
}

interface PremiumRequest {
  id: number
  user_id: string
  email: string
  full_name: string
  plan_type: string
  duration_months: number
  amount_paid: number
  payment_method: string
  payment_proof_url: string
  status: 'pending' | 'approved' | 'rejected'
  created_at: string
  admin_notes: string
}

interface User {
  id: string
  user_id: string
  email: string
  full_name: string
  plan_type: string
  premium_expires_at: string | null
  premium_purchase_date?: string | null
  is_admin: boolean
  created_at: string
}

interface AdminUser {
  id: string
  user_id: string
  email: string
  full_name: string
  is_admin: boolean
  admin_role: string | null
  admin_permissions: any
  role_details?: any
  can_be_deleted?: boolean
  created_at: string
}



interface RateLimitConfig {
  id: string
  setting_name: string
  setting_value: number
  description: string | null
  is_active: boolean
  created_at: string
  updated_at: string
}

export function EnhancedAdminPanel() {
  const { profile, user } = useAuth()
  const { theme } = useTheme()
  const [loading, setLoading] = useState(false)
  const [activeTab, setActiveTab] = useState<'dashboard' | 'requests' | 'users' | 'admins' | 'wallpapers' | 'banners' | 'categories' | 'collections' | 'ratelimits' | 'slugs' | 'ads' | 'cache'>('dashboard')
  const [showMobileMenu, setShowMobileMenu] = useState(false)
  const [stats, setStats] = useState<AdminStats | null>(null)
  const [premiumRequests, setPremiumRequests] = useState<PremiumRequest[]>([])
  const [users, setUsers] = useState<User[]>([])
  const [adminUsers, setAdminUsers] = useState<AdminUser[]>([])
  const [availableRoles, setAvailableRoles] = useState<any[]>([])

  const [rateLimitConfigs, setRateLimitConfigs] = useState<RateLimitConfig[]>([])
  const [editingRateLimit, setEditingRateLimit] = useState<RateLimitConfig | null>(null)
  const [processingRequest, setProcessingRequest] = useState<number | null>(null)
  const [showAuthModal, setShowAuthModal] = useState(false)

  // Check if current user is admin
  const isAdmin = profile?.is_admin === true
  const isAuthenticated = !!user

  useEffect(() => {
    if (isAdmin) {
      loadDashboardData()
    }
  }, [isAdmin, activeTab])

  const loadDashboardData = async () => {
    setLoading(true)
    try {
      if (activeTab === 'dashboard') {
        const { data, error } = await supabase.functions.invoke('admin-dashboard?action=stats', {
          method: 'GET'
        })
        if (error) throw error
        setStats(data.data)
      } else if (activeTab === 'requests') {
        const { data, error } = await supabase.functions.invoke('admin-dashboard?action=premium-requests', {
          method: 'GET'
        })
        if (error) throw error
        setPremiumRequests(data.data)
      } else if (activeTab === 'users') {
        const { data, error } = await supabase.functions.invoke('admin-dashboard?action=users', {
          method: 'GET'
        })
        if (error) throw error
        setUsers(data.data)
      } else if (activeTab === 'admins') {
        const { data, error } = await supabase.functions.invoke('admin-dashboard?action=admins', {
          method: 'GET'
        })
        if (error) throw error
        setAdminUsers(data.data || [])
        setAvailableRoles(data.availableRoles || [])
      } else if (activeTab === 'ratelimits') {
        const { data, error } = await supabase.functions.invoke('admin-rate-limits?action=list', {
          method: 'GET'
        })
        if (error) throw error
        setRateLimitConfigs(data.data || [])
      }
    } catch (error: any) {
      console.error('Error loading data:', error)
      toast.error('Failed to load dashboard data')
    } finally {
      setLoading(false)
    }
  }

  const processPremiumRequest = async (requestId: number, status: 'approved' | 'rejected', adminNotes = '') => {
    setProcessingRequest(requestId)
    try {
      const { data, error } = await supabase.functions.invoke('admin-dashboard', {
        method: 'PUT',
        body: {
          action: 'process-premium-request',
          id: requestId,
          status,
          admin_notes: adminNotes
        }
      })

      if (error) throw error
      
      toast.success(`Request ${status} successfully`)
      loadDashboardData() // Refresh data
    } catch (error: any) {
      console.error('Error processing request:', error)
      toast.error('Failed to process request')
    } finally {
      setProcessingRequest(null)
    }
  }

  const updateUserPremium = async (userId: string, planType: 'free' | 'premium', expiresAt?: string) => {
    try {
      const { data, error } = await supabase.functions.invoke('admin-dashboard', {
        method: 'PUT',
        body: {
          action: 'update-user-premium',
          id: userId,
          status: planType,
          premium_expires_at: expiresAt
        }
      })

      if (error) throw error
      
      toast.success('User premium status updated')
      loadDashboardData() // Refresh data
    } catch (error: any) {
      console.error('Error updating user:', error)
      toast.error('Failed to update user premium status')
    }
  }



  const updateAdminRole = async (userId: string, newRole: string) => {
    try {
      const { data, error } = await supabase.functions.invoke('admin-dashboard', {
        method: 'PUT',
        body: {
          action: 'update-admin-role',
          id: userId,
          admin_role: newRole
        }
      })

      if (error) throw error
      
      toast.success('Admin role updated successfully')
      loadDashboardData() // Refresh data
    } catch (error: any) {
      console.error('Error updating admin role:', error)
      toast.error('Failed to update admin role')
    }
  }

  const deleteAdmin = async (userId: string) => {
    if (!confirm('Are you sure you want to remove this admin user? This action cannot be undone.')) {
      return
    }

    try {
      const { data, error } = await supabase.functions.invoke('admin-dashboard', {
        method: 'DELETE',
        body: {
          action: 'delete-admin',
          id: userId
        }
      })

      if (error) throw error
      
      toast.success('Admin user removed successfully')
      loadDashboardData() // Refresh data
    } catch (error: any) {
      console.error('Error deleting admin:', error)
      toast.error('Failed to remove admin user')
    }
  }

  const promoteToAdmin = async (userId: string, role: string = 'admin') => {
    try {
      const { data, error } = await supabase.functions.invoke('admin-dashboard', {
        method: 'PUT',
        body: {
          action: 'promote-to-admin',
          id: userId,
          admin_role: role
        }
      })

      if (error) throw error
      
      toast.success('User promoted to admin successfully')
      loadDashboardData() // Refresh data
    } catch (error: any) {
      console.error('Error promoting user:', error)
      toast.error('Failed to promote user to admin')
    }
  }



  const updateRateLimitConfig = async (settingName: string, settingValue: number, description?: string, isActive?: boolean) => {
    try {
      const { data, error } = await supabase.functions.invoke('admin-rate-limits', {
        method: 'PUT',
        body: {
          setting_name: settingName,
          setting_value: settingValue,
          description: description || null,
          is_active: isActive !== undefined ? isActive : true
        }
      })

      if (error) throw error
      
      toast.success('Rate limit updated successfully')
      loadDashboardData() // Refresh data
      setEditingRateLimit(null)
    } catch (error: any) {
      console.error('Error updating rate limit:', error)
      toast.error('Failed to update rate limit configuration')
    }
  }

  // Show login form if not authenticated
  if (!isAuthenticated) {
    return (
      <div className="min-h-screen flex items-center justify-center bg-theme-primary">
        <div className="text-center max-w-md mx-auto p-6">
          <Settings className="w-16 h-16 mx-auto mb-4 text-theme-tertiary" />
          <h1 className="text-2xl font-bold mb-2 text-theme-primary">Admin Login Required</h1>
          <p className="text-theme-secondary mb-6">Please sign in with your admin credentials to access the admin panel.</p>
          <button
            onClick={() => setShowAuthModal(true)}
            className="w-full bg-indigo-600 text-white px-6 py-3 rounded-lg hover:bg-indigo-700 transition-colors"
          >
            Sign In
          </button>
          {showAuthModal && (
            <AuthModal 
              isOpen={showAuthModal} 
              onClose={() => setShowAuthModal(false)}
              initialMode="login"
            />
          )}
        </div>
      </div>
    )
  }

  // Show access denied if authenticated but not admin
  if (!isAdmin) {
    return (
      <div className="min-h-screen flex items-center justify-center bg-theme-primary">
        <div className="text-center">
          <Settings className="w-16 h-16 mx-auto mb-4 text-theme-tertiary" />
          <h1 className="text-2xl font-bold mb-2 text-theme-primary">Admin Access Required</h1>
          <p className="text-theme-secondary">You need admin privileges to access this panel.</p>
          <p className="text-sm text-theme-tertiary mt-2">Contact an administrator to request admin access.</p>
        </div>
      </div>
    )
  }

  return (
    <div className="min-h-screen bg-theme-secondary">
      {/* Header */}
      <div className="shadow bg-theme-card border-b border-theme-light">
        <div className="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-6">
          <div className="flex items-center space-x-3">
            <Settings className="w-8 h-8 text-gray-600" />
            <div>
              <h1 className="text-2xl font-bold text-theme-primary">
                Enhanced Admin Dashboard
              </h1>
              <p className="text-theme-secondary">
                Comprehensive management system
              </p>
            </div>
          </div>
        </div>
      </div>

      <div className="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
        {/* Tabs */}
        <div className="mb-8">
          <div className="border-b border-theme-light">
            {/* Desktop Navigation */}
            <nav className="hidden lg:block -mb-px">
              {/* Content Section */}
              <div className="mb-6">
                <h3 className="text-sm font-medium text-theme-secondary mb-3 px-1">Content Management</h3>
                <div className="flex flex-wrap gap-x-8 gap-y-2">
                  {[
                    { id: 'wallpapers', name: 'Free Wallpapers', icon: ImageIcon },
                    { id: 'categories', name: 'Categories', icon: Tag },
                    { id: 'collections', name: 'Collections', icon: ImageIcon },
                    { id: 'slugs', name: 'Slugs', icon: Link },
                  ].map((tab) => (
                    <button
                      key={tab.id}
                      onClick={() => setActiveTab(tab.id as any)}
                      className={`flex items-center space-x-2 py-2 px-3 border-b-2 font-medium text-sm transition-colors rounded-t-lg ${
                        activeTab === tab.id
                          ? 'border-gray-500 text-gray-600 bg-theme-light'
                          : 'border-transparent text-theme-secondary hover:text-theme-primary hover:border-theme-light hover:bg-theme-surface'
                      }`}
                    >
                      <tab.icon className="w-5 h-5" />
                      <span>{tab.name}</span>
                    </button>
                  ))}
                </div>
              </div>
              
              {/* Monetization Section */}
              <div className="mb-6">
                <h3 className="text-sm font-medium text-theme-secondary mb-3 px-1">Monetization</h3>
                <div className="flex flex-wrap gap-x-8 gap-y-2">
                  {[
                    { id: 'ads', name: 'Ad Settings', icon: Zap },
                    { id: 'banners', name: 'Premium Banners', icon: Megaphone },
                  ].map((tab) => (
                    <button
                      key={tab.id}
                      onClick={() => setActiveTab(tab.id as any)}
                      className={`flex items-center space-x-2 py-2 px-3 border-b-2 font-medium text-sm transition-colors rounded-t-lg ${
                        activeTab === tab.id
                          ? 'border-gray-500 text-gray-600 bg-theme-light'
                          : 'border-transparent text-theme-secondary hover:text-theme-primary hover:border-theme-light hover:bg-theme-surface'
                      }`}
                    >
                      <tab.icon className="w-5 h-5" />
                      <span>{tab.name}</span>
                    </button>
                  ))}
                </div>
              </div>
              
              {/* User Management Section */}
              <div className="mb-6">
                <h3 className="text-sm font-medium text-theme-secondary mb-3 px-1">User Management</h3>
                <div className="flex flex-wrap gap-x-8 gap-y-2">
                  {[
                    { id: 'users', name: 'User Management', icon: Users },
                    { id: 'requests', name: 'Premium Requests', icon: Crown },
                    { id: 'admins', name: 'Admin Management', icon: Settings },
                  ].map((tab) => (
                    <button
                      key={tab.id}
                      onClick={() => setActiveTab(tab.id as any)}
                      className={`flex items-center space-x-2 py-2 px-3 border-b-2 font-medium text-sm transition-colors rounded-t-lg ${
                        activeTab === tab.id
                          ? 'border-gray-500 text-gray-600 bg-theme-light'
                          : 'border-transparent text-theme-secondary hover:text-theme-primary hover:border-theme-light hover:bg-theme-surface'
                      }`}
                    >
                      <tab.icon className="w-5 h-5" />
                      <span>{tab.name}</span>
                    </button>
                  ))}
                </div>
              </div>
              
              {/* System Section */}
              <div className="mb-6">
                <h3 className="text-sm font-medium text-theme-secondary mb-3 px-1">System & Performance</h3>
                <div className="flex flex-wrap gap-x-8 gap-y-2">
                  {[
                    { id: 'dashboard', name: 'Dashboard', icon: TrendingUp },
                    { id: 'ratelimits', name: 'Rate Limits', icon: Gauge },
                    { id: 'cache', name: 'Cache & Performance', icon: Server },
                  ].map((tab) => (
                    <button
                      key={tab.id}
                      onClick={() => setActiveTab(tab.id as any)}
                      className={`flex items-center space-x-2 py-2 px-3 border-b-2 font-medium text-sm transition-colors rounded-t-lg ${
                        activeTab === tab.id
                          ? 'border-gray-500 text-gray-600 bg-theme-light'
                          : 'border-transparent text-theme-secondary hover:text-theme-primary hover:border-theme-light hover:bg-theme-surface'
                      }`}
                    >
                      <tab.icon className="w-5 h-5" />
                      <span>{tab.name}</span>
                    </button>
                  ))}
                </div>
              </div>
            </nav>
            
            {/* Mobile Navigation - Dropdown */}
            <div className="lg:hidden relative">
              <button
                onClick={() => setShowMobileMenu(!showMobileMenu)}
                className="flex items-center justify-between w-full py-3 px-4 bg-theme-surface border border-theme-light rounded-lg text-theme-primary font-medium"
              >
                <div className="flex items-center space-x-2">
                  {[...[
                    { id: 'wallpapers', name: 'Free Wallpapers', icon: ImageIcon },
                    { id: 'categories', name: 'Categories', icon: Tag },
                    { id: 'collections', name: 'Collections', icon: ImageIcon },
                    { id: 'slugs', name: 'Slugs', icon: Link },
                    { id: 'ads', name: 'Ad Settings', icon: Zap },
                    { id: 'banners', name: 'Premium Banners', icon: Megaphone },
                    { id: 'users', name: 'User Management', icon: Users },
                    { id: 'requests', name: 'Premium Requests', icon: Crown },
                    { id: 'admins', name: 'Admin Management', icon: Settings },
                    { id: 'dashboard', name: 'Dashboard', icon: TrendingUp },
                    { id: 'ratelimits', name: 'Rate Limits', icon: Gauge },
                    { id: 'cache', name: 'Cache & Performance', icon: Server },
                  ]].find(tab => tab.id === activeTab)?.icon && (
                    <>
                      {React.createElement([...[
                        { id: 'wallpapers', name: 'Free Wallpapers', icon: ImageIcon },
                        { id: 'categories', name: 'Categories', icon: Tag },
                        { id: 'collections', name: 'Collections', icon: ImageIcon },
                        { id: 'slugs', name: 'Slugs', icon: Link },
                        { id: 'ads', name: 'Ad Settings', icon: Zap },
                        { id: 'banners', name: 'Premium Banners', icon: Megaphone },
                        { id: 'users', name: 'User Management', icon: Users },
                        { id: 'requests', name: 'Premium Requests', icon: Crown },
                        { id: 'admins', name: 'Admin Management', icon: Settings },
                        { id: 'dashboard', name: 'Dashboard', icon: TrendingUp },
                        { id: 'ratelimits', name: 'Rate Limits', icon: Gauge },
                        { id: 'cache', name: 'Cache & Performance', icon: Server },
                      ]].find(tab => tab.id === activeTab)?.icon || TrendingUp, { className: "w-5 h-5" })}
                      <span>{[
                        { id: 'wallpapers', name: 'Free Wallpapers' },
                        { id: 'categories', name: 'Categories' },
                        { id: 'collections', name: 'Collections' },
                        { id: 'slugs', name: 'Slugs' },
                        { id: 'ads', name: 'Ad Settings' },
                        { id: 'banners', name: 'Premium Banners' },
                        { id: 'users', name: 'User Management' },
                        { id: 'requests', name: 'Premium Requests' },
                        { id: 'admins', name: 'Admin Management' },
                        { id: 'dashboard', name: 'Dashboard' },
                        { id: 'ratelimits', name: 'Rate Limits' },
                        { id: 'cache', name: 'Cache & Performance' },
                      ].find(tab => tab.id === activeTab)?.name || 'Dashboard'
                      }</span>
                    </>
                  )}
                </div>
                <ChevronDown className={`w-5 h-5 transform transition-transform ${showMobileMenu ? 'rotate-180' : ''}`} />
              </button>
              
              {showMobileMenu && (
                <div className="absolute top-full left-0 right-0 mt-1 bg-theme-surface border border-theme-light rounded-lg shadow-lg z-50 max-h-96 overflow-y-auto">
                  {/* Content Management Section */}
                  <div className="border-b border-theme-light">
                    <div className="px-4 py-2 bg-theme-muted">
                      <h4 className="text-xs font-semibold text-theme-secondary uppercase tracking-wider">Content Management</h4>
                    </div>
                    {[
                      { id: 'wallpapers', name: 'Free Wallpapers', icon: ImageIcon },
                      { id: 'categories', name: 'Categories', icon: Tag },
                      { id: 'collections', name: 'Collections', icon: ImageIcon },
                      { id: 'slugs', name: 'Slugs', icon: Link },
                    ].map((tab) => (
                      <button
                        key={tab.id}
                        onClick={() => {
                          setActiveTab(tab.id as any)
                          setShowMobileMenu(false)
                        }}
                        className={`flex items-center space-x-3 w-full py-3 px-4 text-left transition-colors ${
                          activeTab === tab.id
                            ? 'bg-theme-light text-gray-600'
                            : 'text-theme-secondary hover:bg-theme-light hover:text-theme-primary'
                        }`}
                      >
                        <tab.icon className="w-5 h-5" />
                        <span className="font-medium">{tab.name}</span>
                      </button>
                    ))}
                  </div>
                  
                  {/* Monetization Section */}
                  <div className="border-b border-theme-light">
                    <div className="px-4 py-2 bg-theme-muted">
                      <h4 className="text-xs font-semibold text-theme-secondary uppercase tracking-wider">Monetization</h4>
                    </div>
                    {[
                      { id: 'ads', name: 'Ad Settings', icon: Zap },
                      { id: 'banners', name: 'Premium Banners', icon: Megaphone },
                    ].map((tab) => (
                      <button
                        key={tab.id}
                        onClick={() => {
                          setActiveTab(tab.id as any)
                          setShowMobileMenu(false)
                        }}
                        className={`flex items-center space-x-3 w-full py-3 px-4 text-left transition-colors ${
                          activeTab === tab.id
                            ? 'bg-theme-light text-gray-600'
                            : 'text-theme-secondary hover:bg-theme-light hover:text-theme-primary'
                        }`}
                      >
                        <tab.icon className="w-5 h-5" />
                        <span className="font-medium">{tab.name}</span>
                      </button>
                    ))}
                  </div>
                  
                  {/* User Management Section */}
                  <div className="border-b border-theme-light">
                    <div className="px-4 py-2 bg-theme-muted">
                      <h4 className="text-xs font-semibold text-theme-secondary uppercase tracking-wider">User Management</h4>
                    </div>
                    {[
                      { id: 'users', name: 'User Management', icon: Users },
                      { id: 'requests', name: 'Premium Requests', icon: Crown },
                      { id: 'admins', name: 'Admin Management', icon: Settings },
                    ].map((tab) => (
                      <button
                        key={tab.id}
                        onClick={() => {
                          setActiveTab(tab.id as any)
                          setShowMobileMenu(false)
                        }}
                        className={`flex items-center space-x-3 w-full py-3 px-4 text-left transition-colors ${
                          activeTab === tab.id
                            ? 'bg-theme-light text-gray-600'
                            : 'text-theme-secondary hover:bg-theme-light hover:text-theme-primary'
                        }`}
                      >
                        <tab.icon className="w-5 h-5" />
                        <span className="font-medium">{tab.name}</span>
                      </button>
                    ))}
                  </div>
                  
                  {/* System & Performance Section */}
                  <div>
                    <div className="px-4 py-2 bg-theme-muted">
                      <h4 className="text-xs font-semibold text-theme-secondary uppercase tracking-wider">System & Performance</h4>
                    </div>
                    {[
                      { id: 'dashboard', name: 'Dashboard', icon: TrendingUp },
                      { id: 'ratelimits', name: 'Rate Limits', icon: Gauge },
                      { id: 'cache', name: 'Cache & Performance', icon: Server },
                    ].map((tab) => (
                      <button
                        key={tab.id}
                        onClick={() => {
                          setActiveTab(tab.id as any)
                          setShowMobileMenu(false)
                        }}
                        className={`flex items-center space-x-3 w-full py-3 px-4 text-left transition-colors ${
                          activeTab === tab.id
                            ? 'bg-theme-light text-gray-600'
                            : 'text-theme-secondary hover:bg-theme-light hover:text-theme-primary'
                        }`}
                      >
                        <tab.icon className="w-5 h-5" />
                        <span className="font-medium">{tab.name}</span>
                      </button>
                    ))}
                  </div>
                </div>
              )}
            </div>
          </div>
        </div>

        {/* Categories Tab */}
        {activeTab === 'categories' && (
          <div>
            <CategoriesManagement />
          </div>
        )}

        {/* Collections Tab */}
        {activeTab === 'collections' && (
          <div>
            <CollectionsManagement />
          </div>
        )}

        {/* Slug Management Tab */}
        {activeTab === 'slugs' && (
          <div>
            <SlugManagement />
          </div>
        )}

        {/* Ad Settings Tab */}
        {activeTab === 'ads' && (
          <div>
            <AdSettingsManagement />
          </div>
        )}

        {/* Banners Tab */}
        {activeTab === 'banners' && (
          <div>
            <BannerManagement />
          </div>
        )}

        {/* Dashboard Tab */}
        {activeTab === 'dashboard' && (
          <div>
            {/* Stats Cards */}
            {stats && (
              <div className="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-5 gap-6 mb-8">
                <div className="rounded-lg shadow p-6 bg-theme-surface border border-theme-light">
                  <div className="flex items-center">
                    <Users className="w-8 h-8 text-blue-600" />
                    <div className="ml-4">
                      <p className="text-sm font-medium text-theme-secondary">Total Users</p>
                      <p className="text-2xl font-semibold text-theme-primary">{stats.totalUsers}</p>
                      <p className="text-xs text-green-600">+{stats.newUsersLast30Days} this month</p>
                    </div>
                  </div>
                </div>
                
